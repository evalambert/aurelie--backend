{
  "kind": "collectionType",
  "collectionName": "atlases",
  "info": {
    "singularName": "atlas",
    "pluralName": "atlases",
    "displayName": "Atlas",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "technique": {
      "type": "string"
    },
    "format": {
      "type": "string"
    },
    "duration": {
      "type": "string"
    },
    "origin": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "year": {
      "type": "integer"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "exhibition": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::exhibition.exhibition",
      "inversedBy": "atlasRelation"
    },
    "medium": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::medium.medium",
      "mappedBy": "atlas"
    },
    "parents": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::atlas.atlas",
      "inversedBy": "siblings"
    },
    "siblings": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::atlas.atlas",
      "mappedBy": "parents"
    },
    "oembedVideo": {
      "type": "customField",
      "customField": "plugin::oembed.oembed"
    },
    "shortVideo": {
      "allowedTypes": [
        "files",
        "videos"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
